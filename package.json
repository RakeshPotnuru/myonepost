{
  "name": "1post",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "frontend": "turbo dev --filter=@1post/frontend",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky"
  },
  "devDependencies": {
    "@bravo68web/eslint-config": "^1.2.5",
    "husky": "^9.1.6",
    "prettier": "^3.2.5",
    "turbo": "^2.1.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
